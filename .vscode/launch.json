    "version": "0.2.0",
    "configurations": [
    {
        "name": "Launch Edge",
        "request": "launch",
        "type": "msedge",
        "url": "http://localhost:5175",
        "webRoot": "${workspaceFolder}/dashboard/",
        "sourceMapPathOverrides": {
            "webpack:///src/*": "${webRoot}/*"
        },
        
        
    },
    {
        "type": "chrome",
        "request": "launch",
        "name": "Vue: Chrome",
        "url": "http://localhost:8080", // 确保与开发服务器的 URL 一致
        "webRoot": "${workspaceFolder}/src",
        "sourceMapPathOverrides": {
          "webpack:///src/*": "${webRoot}/*"
        }
      }
    ]
}